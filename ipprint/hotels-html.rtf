\par
  #apply button\{\par
    margin: 2px 0px 0px -5px;\par
    color: azure;\par
    font-size: 15px;\par
    border: none;\par
    font-family: 'Raleway';\par
    font-weight: bold;\par
    padding: 11px 10px;\par
    background-color: var(--primary-color);\par
