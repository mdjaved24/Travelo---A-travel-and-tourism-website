\}\par
\par
.hotel-box h6\{\par
  margin: 3px;\par
  padding: 0;\par
  font-family: 'Ubuntu';\par
  font-size: 18px;\par
  text-align: center;\par
  color: black;\par
\}\par
\par
.hotel-box p\{\par
  margin: 0px 0px 0px 25px;\par
  padding: 0px;\par
  font-family: 'Ubuntu';\par
  font-size: 15px;\par
  color: black;\par
\}\par
\par
.hotel-box p strong\{\par
  color: red;\par
  font-size: 17px;\par
  font-family: 'Borol',cursive;\par
\}\par
\par
  /* SORT */\par
\par
#item1\{\par
  margin: -25px 25px 0px -75px;\par
  width: 10%;\par
\}\par
\par
  #sort\{\par
    display: flex;\par
    flex-direction: column;\par
    flex-wrap: wrap;\par
    justify-content: center;\par
    width: 100%;\par
