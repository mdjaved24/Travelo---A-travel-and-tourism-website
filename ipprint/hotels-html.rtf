\par
  .box\{\par
    border: 2px solid black;\par
    width: 360px;\par
    height: 360px;\par
  \}\par
  /* --------------------------------------------*/\par
  /* FOOTER  */\par
\par
  footer\{\par
    background: linear-gradient(rgba(0, 0, 0, 0.9),rgba(0, 0, 0, 0.9));\par
