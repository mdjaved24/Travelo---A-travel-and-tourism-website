/* FONTS */\par
@import url('https://fonts.googleapis.com/css2?family=Borel&family=Raleway:wght@500&family=Ubuntu:wght@300&display=swap');\par
\par
/* Variables */\par
:root\{\par
    --primary-color:rgba(0, 155, 155, 0.697);\par
    --secondary-color:rgb(247, 250, 251);\par
\}\par
/* NAVIGATION BAR  */\par
nav\{\par
    margin: 20px 0px 40px 0px;\par
    height: 90px;\par
    border-radius: 8px;\par
    background-color: var(--secondary-color);\par
    border-radius: 6px;\par
    box-shadow: 0 1px 30px 0 rgba(0,0,0,.1);\par
  \}\par
\par
nav ul\{\par
    overflow: auto;\par
    margin: 25px 10px;\par
  \}\par
\par
 .nav\{\par
    float: left;\par
    list-style: none;\par
    margin: 25px 10px;\par
  \}\par
\par
  /* Styling links */\par
 .nav a\{\par
  margin: 0;\par
    padding: 8px;\par
    text-decoration: none;\par
    color: black;\par
    font-size: 22px;\par
    font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;\par
  \}\par
\par
  .nav a:hover,.nav a:active,.nav a:focus\{\par
    color: var(--primary-color);\par
    border-bottom: 2px solid var(--primary-color);\par
  \}\par
\par
  #hotels\{\par
    color: var(--primary-color);\par
    border-bottom: 2px solid var(--primary-color);\par
  \}\par
\par
  /* Search bar */\par
  #search\{\par
    display: flex;\par
    align-items: center;\par
    float: right;\par
    margin: 25px 10px;\par
    padding: 5px;\par
    background-color: white;\par
    border: 2px solid black;\par
    border-radius: 20px;\par
    width: 245px;\par
    height: 25px;\par
  \}\par
\par
  #search input\{\par
    border: none;\par
    width: 220px;\par
    height: 25px;\par
  \}\par
\par
  #img\{\par
    width: 20px;\par
    height: 20px;\par
  \}\par
\par
\par
  /* ---------------------------------------------*/\par
   /* HOTELHEAD */\par
   .hotelhead\{\par
    margin: 0;\par
    padding: 0;\par
    background: url('Images/hotelhead.jpg');\par
    background-position: center;\par
    background-size: 100%;\par
    background-repeat: no-repeat;\par
    height: 250px;\par
    border-radius: 10px;\par
    box-shadow: 2px 2px 2px 2px var(--secondary-color);\par
  \}\par
  .hotelhead h6\{\par
    margin: 0;\par
    padding: 70px;\par
    font-size: 90px;\par
    text-align: center;\par
    font-family: 'Ubuntu';\par
    color: white;\par
  \}\par
\par
  /* HOTEL FORM */\par
  form\{\par
    margin: 20px;\par
    padding: 20px;\par
    display: flex;\par
    flex-direction: row;\par
    flex-wrap: wrap;\par
    background-color: rgb(247, 250, 251);\par
    border-radius: 6px;\par
    box-shadow: 0 1px 30px 0 rgba(0,0,0,.1);\par
    border-bottom: 3px solid var(--primary-color);\par
  \}\par
\par
  label\{\par
    margin: 0px 0px 0px 40px;\par
    padding: 0;\par
    font-family: 'Raleway', sans-serif;\par
    font-size: 20px;\par
  \}\par
\par
  #form input\{\par
    margin: -150px 0px 0px 0px;\par
    width: 150px;\par
    height: 30px;\par
    font-family: 'Raleway';\par
    font-size: 15px;\par
    border-radius: 10px;\par
  \}\par
\par
  #hotel-name input\{\par
     width: 300px;\par
  \}\par
\par
\par
  #submit-form button\{\par
    margin: 65px 0px 0px 20px;\par
    color: azure;\par
    font-size: 15px;\par
    border: none;\par
    font-family: 'Raleway';\par
    font-weight: bold;\par
    padding: 11px 10px;\par
    background-color: var(--primary-color);\par
    text-align: center;\par
    width: 140px;\par
    color: #fff;\par
    box-shadow: 4px 4px 0px 0px rgba(0, 0, 0, 0.4) inset;\par
    cursor: pointer;\par
  \}\par
\par
  #submit-form button:hover\{\par
    background-color: black;\par
  \}\par
\par
/* --------------------------------------------------------*/\par
  /* CONTAINER  */\par
\par
.container\{\par
    display: flex;\par
    flex-direction: row;\par
    flex-wrap: wrap;\par
    justify-content: space-evenly;\par
    margin: 22px;\par
\}\par
\par
.box\{\par
    width: 360px;\par
    height: 360px;\par
    margin: 10px 10px 10px 20px;\par
\}\par
\par
#item2\{\par
    width: 85%;\par
    margin: -5px -50px 0px 30px;\par
    display: flex;\par
    flex-direction: row;\par
    flex-wrap: wrap;\par
    justify-content: space-evenly;\par
    background-color: rgb(247, 250, 251);\par
    border-radius: 6px;\par
    box-shadow: 0 1px 30px 0 rgba(0,0,0,.1);\par
    border-bottom: 3px solid var(--primary-color);\par
    border-right: 3px solid var(--primary-color);\par
    border-top: none;\par
    border-left: none;\par
\}\par
\par
.hotel-box\{\par
  width: 230px;\par
  height: 230px;\par
  margin: 10px 30px;\par
  border: 1px solid var(--secondary-color);\par
  margin: 10px;\par
  border-radius: 10px;\par
  box-shadow: 0 1px 30px 0 rgba(0,0,0,.1);\par
  cursor: pointer;\par
\}\par
\par
.hotel-box:hover\{\par
  box-shadow: 0 1px 34px 2px rgba(0,0,0,.2);\par
\}\par
\par
.hotel-box img\{\par
  margin: 0;\par
  border-radius: 10px;\par
